datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

enum Sport {
  BASEBALL
  BASKETBALL
  DODGEBALL
  FOOTBALL
  LACROSSE
  SOFTBALL
}

model League {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  sport       String
}
